{
  "name": "constructor-ui-rossery",
  "version": "0.1.9",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "homepage": "http://r1tsuu.github.io/constructor-ui",
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2"
    ]
  },
  "dependencies": {
    "@storybook/preview-web": "^6.5.13",
    "@storybook/router": "^6.5.13",
    "bson": "^4.7.0",
    "clsx": "1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "3.1.1",
    "react-transition-group": "4.4.5",
    "swiper": "8.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/ --out-dir dist --copy-files --ignore __tests__,.storybook,spec.js,test.js,.stories.js,__snapshots__",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/manager-webpack4": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "0.0.13",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "sass": "1.55.0"
  }
}
